<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="
        http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd" xmlns:p="http://www.springframework.org/schema/p">

	<bean name="Emp1" class="com.SpringFramework.ConstructorInjection.ConstructorClass">
		<constructor-arg>
			<value>Harsh</value>
		</constructor-arg>
		<constructor-arg>
			<value>800000</value>
		</constructor-arg>
		<constructor-arg>
			<value>Delhi</value>
		</constructor-arg>
		<constructor-arg>
			<list>
				<value>Komal</value>
				<value>Rekha</value>
				<value>Uzma</value>
			</list>
		</constructor-arg>
	</bean>


	<bean id="Emp2" class="com.SpringFramework.ConstructorInjection.ConstructorClass">
		<constructor-arg value="Seema Yadav"></constructor-arg>
		<constructor-arg value="900000"></constructor-arg>
		<constructor-arg value="Faridabad"></constructor-arg>
	</bean>

</beans>