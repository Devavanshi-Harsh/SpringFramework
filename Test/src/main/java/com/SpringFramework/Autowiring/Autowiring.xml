<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:context="http://www.springframework.org/schema/context" xsi:schemaLocation="
        http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd
        http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context.xsd">
	<context:annotation-config />
	<bean class="com.SpringFramework.Autowiring.files.Address" name="address1">
		<property name="city">
			<value>Delhi</value>
		</property>
		<property name="district">
			<value>Najafgarh</value>
		</property>
	</bean>
	<bean class="com.SpringFramework.Autowiring.files.Address" name="address2">
		<property name="city">
			<value>Kanpur</value>
		</property>
		<property name="district">
			<value>Shuklaganj</value>
		</property>
	</bean>
	<bean class="com.SpringFramework.Autowiring.files.AutowiringClass" name="person" autowire="byType" />
	<bean class="com.SpringFramework.Autowiring.files.AnnotationAutowiring" name="autowire"/>

</beans>