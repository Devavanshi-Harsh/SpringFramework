<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="
        http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd" xmlns:p="http://www.springframework.org/schema/p">

	<bean class="com.SpringFramework.Autowiring.files.Address" name="address">
		<property name="city">
			<value>Delhi</value>
		</property>
		<property name="district">
			<value>Najafgarh</value>
		</property>
	</bean>
	<bean class="com.SpringFramework.Autowiring.files.AutowiringClass" name="person" autowire="byType" />
</beans>