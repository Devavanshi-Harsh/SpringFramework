<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="
        http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd" xmlns:p="http://www.springframework.org/schema/p">

	<bean id="add1" class="com.SpringFramework.Ambiguity.Ambiguity">
		<constructor-arg>
			<value>10</value>
		</constructor-arg>
		<constructor-arg>
			<value>20</value>
		</constructor-arg>
	</bean>


	<bean id="add2" class="com.SpringFramework.Ambiguity.Ambiguity">
		<constructor-arg value="50" type = "int" />
		<constructor-arg value="60" type = "int" />
	</bean>
<bean id="add3" class="com.SpringFramework.Ambiguity.Ambiguity">
		<constructor-arg value="50" type = "double" />
		<constructor-arg value="60" type = "double" />
	</bean>

</beans>